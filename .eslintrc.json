{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": 0,
        "object-curly-newline": 0,
        "no-underscore-dangle": 0
    }
}
